SELECT 
    Brand,
    SUM(Kuantitas) AS total_qty_terjual
FROM transactions
GROUP BY Brand
ORDER BY total_qty_terjual DESC;


SELECT 
    ID_Customer,
    SUM(Grand_Total) AS total_belanja
FROM transactions
GROUP BY ID_Customer
ORDER BY total_belanja DESC
LIMIT 10;


SELECT 
    Product,
    Harga
FROM transactions
WHERE Brand = 'Scriba';



SELECT DISTINCT
    Product,
    Harga
FROM transactions
ORDER BY CAST(REPLACE(Harga, ',', '') AS INTEGER) ASC
LIMIT 10;

